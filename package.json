{
  "name": "homotopy",
  "main": "./lib/homotopy",
  "version": "0.1.3",
  "description": "Snippet compiler",
  "keywords": [],
  "activationCommands": {
    "atom-workspace": ["homotopy:expand", "homotopy:expand-into"]
  },
  "repository": "https://github.com/Ahhhhmed/homotopy-atom",
  "homepage": "https://github.com/Ahhhhmed/homotopy-atom",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {},
  "configSchema": {
    "Homotopy path": {
      "type": "string",
      "default": "homotopy",
      "description": "If `homotopy` is not in your PATH, specify where the executable is",
      "order": 1
    },
    "User lib path": {
      "type": "array",
      "default": [],
      "description": "Path to user library folders. Comma separated",
      "items": {
        "type": "string"
      },
      "order": 2
    }
  }
}
